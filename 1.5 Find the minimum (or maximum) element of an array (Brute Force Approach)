import java.util.*;

class Main{
    public static void main(String[] args){
        int[] arr = {1,2,3,4,5,6,7};
        int n = arr.length;
        
        int max = Integer.MIN_VALUE;
        int min = Integer.MAX_VALUE;
        
        for(int i=0;i<n;i++){
            int ele = arr[i];
            if(ele > max){
                max = ele;
            }
            if(ele < min){
                min = ele;
            }
        }
        
        System.out.print("max: " + max + ", " + "min: " + min);
    }
}
