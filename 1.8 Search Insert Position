import java.util.*;

class Main{
    public static void main(String[] args){
        int[] arr = {1,1,2,5,9,12};
        int n = arr.length;
        int k = 7;
        
        int index = -1;
        for(int i=0;i<n;i++){
            if(arr[i] == k){
                index = i;
                break;
            }
            else if(arr[i] > k){
                index = i;
                break;
            }
        }
        
        System.out.println(index);
    }
}
